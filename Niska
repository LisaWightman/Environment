. ~/Environment/git
. ~/Environment/git-completion.bash
. ~/Environment/mac_shortcuts
. ~/Environment/android

alias lc="adb logcat | grep -E -v 'dalvikvm|CachedRequest|DETools\.|V/Wifi|wpa_supplicant'"
alias bounce="adb kill-server; adb start-server"

ormlite_log() {
	local level=$1
	adb -d shell setprop log.tag.StatementExecutor $1
	adb -d shell setprop log.tag.BaseMappedStatement $1
	adb -d shell setprop log.tag.MappedCreate $1
}

alias mls_un='adb uninstall com.mlssoccer'
alias mls_db='adb pull /data/data/com.mlssoccer/databases/mls.db .'
mls_clean() {
    [ -d mls ] || { echo "This isn't the mls directory"; return; }
    for s in $(find . -maxdepth 1 -type  d); do
        pushd $s > /dev/null
        [ -f build.xml ] && ant clean -Dsdk.dir=/Users/jvinding/Tools/android/
        popd > /dev/null
    done
}
