alias wifi_ip='ifconfig en0 | grep "\<inet\>" | awk "{print \$2}"'
alias copy_wifi_ip='wifi_ip | tr -d '[:space:]' | pbcopy'


pui() {
    ssh bucket1 "jvinding/user_info.sh $@" | pbcopy
    echo "Info copied to pastboard"
}
